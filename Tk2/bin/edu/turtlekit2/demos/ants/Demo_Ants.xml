<?xml version="1.0" encoding="UTF-8"?>
	<!-- A simple creation simulation -->
<simulation name="Creation Demo" author="LIRMM">
	<environment width="100" height="100" torusMode="true"/>
	<pheromone diffusion="0.08" evaporation="0.01" name="nestScent"/>
	<pheromone diffusion="0.08" evaporation="0.01" name="foodScent"/>
	<pheromone name="food"/>
	<pheromone name="foodreturned"/>
	<turtle class="edu.turtlekit2.demos.ants.Ant" number="500" x="50" y="50"/>
	<observer class="edu.turtlekit2.tools.pheromone3D.Phero3DViewer" pheromone="foodScent"/>
	<observer class="edu.turtlekit2.demos.ants.FoodInitializer" foodX="80" foodY="80"/>
	<observer class="edu.turtlekit2.tools.chart.LineCharter" pheromone="foodreturned"/>
</simulation>
