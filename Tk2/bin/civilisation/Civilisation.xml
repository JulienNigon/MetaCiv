<?xml version="1.0" encoding="UTF-8"?>
<!--Simulation de civilisations-->
<simulation Name="Civilisation">
	<environment class = "civilisation.world.World" width="500" height="300" torusMode="true" />
    

    <pheromone name="passage" diffusion="0.0" evaporation="0.0"/>
    
    <!-- Turtle used to generate other turtles -->
    <turtle class="civilisation.TurtleGenerator" number="1"></turtle>
	
    <observer class="civilisation.world.WorldViewer"  GraphismesAmeliores = "true"/>
	<observer class="civilisation.world.WorldCtrl" />
	
	<observer class="civilisation.inspecteur.viewer.ViewerAgent" />
	<observer class="civilisation.inspecteur.viewer.ViewerCommunaute" />
	<observer class="civilisation.inspecteur.viewer.ViewerOptions" />
	<observer class="civilisation.inspecteur.viewer.ViewerPerformances" />
	<observer class="civilisation.inspecteur.viewer.ViewerModificationSimulation" />
	<observer class="civilisation.inspecteur.viewer.ViewerTableauDeBord" />
	<observer class="edu.turtlekit2.tools.chart.LineCharter" role="Humain"/>
	
	
	<!--  
	<turtle class="civilisation.Communaute" number="6"></turtle>
	<observer class="civilisation.inspecteur.CognitonsCharter"/>
	-->
</simulation>
